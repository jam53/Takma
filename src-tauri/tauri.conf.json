{
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "beforeBundleCommand": ".\\Dependencies\\upx.exe --ultra-brute .\\src-tauri\\target\\release\\Takma.exe",
        "devPath": "http://localhost:1420",
        "distDir": "../dist",
        "withGlobalTauri": false
    },
    "package": {
        "productName": "Takma",
        "version": "0.0.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            },
            "fs": {
                "all": true,
                "scope": [
                    "**"
                ]
            },
            "dialog": {
                "all": true
            },
            "path": {
                "all": true
            }
        },
        "bundle": {
            "active": true,
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "com.jam54.Takma",
            "targets": "all"
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "Takma 0.0.0 (112)",
                "width": 1600,
                "height": 800
            }
        ]
    }
}
