{
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist",
        "devUrl": "http://localhost:1420"
    },
    "bundle": {
        "active": true,
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "resources": [
            "resources/*"
        ],
        "targets": "all"
    },
    "productName": "Takma",
    "mainBinaryName": "Takma",
    "version": "1.5.3",
    "identifier": "com.jam54.Takma",
    "plugins": {},
    "app": {
        "withGlobalTauri": false,
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "Takma 1.5.3 (620)",
                "width": 1600,
                "height": 800,
                "decorations": false,
                "transparent": true,
                "dragDropEnabled": true
            }
        ],
        "security": {
            "assetProtocol": {
                "scope": [
                    "**"
                ],
                "enable": true
            },
            "csp": null
        }
    }
}
